#include <iostream>
#include <string>
using namespace std;

int main()
{
	int number[20];

	for (int i = 0; i < 20; i++) {
		cout << "[" << i + 1 << "]" << ": ";
		cin >> number[i];
	}
	cout << "Your array: ";

	for (int i = 0; i < 20; ++i) {
		cout << number[i] << " ";
	}

	cout << endl;
	system("pause");

	return 0;
}
