#include <iostream>
using namespace std;
int main()
{
	setlocale(0, "");
	int a, b;
	cout << "Введите число:";
	cin >> a;
	cout << "Введите число:";
	cin >> b;
	int R = a - b;
	cout << "Разность a и b = " << R << endl;
	int d, f;
	cout << "Введите число:";
	cin >> d;
	cout << "Введите число:";
	cin >> f;
	int CH = d / f;
	cout << "Частное d и f = " << CH << endl;
	return 0;
}
